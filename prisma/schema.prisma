generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id          String @id @default(uuid())
  name        String
  description String
}

model University {
  id          String @id @default(uuid())
  name        String
  description String
}

model Company {
  id          String @id @default(uuid())
  name        String
  description String
}
